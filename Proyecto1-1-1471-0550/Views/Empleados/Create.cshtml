@model Proyecto1_1_1471_0550.Models.Empleado

<h2>Nuevo Empleado</h2>

<form asp-action="Create">
    <div class="form-group">
        <label>Cédula</label>
        <input asp-for="Cedula" class="form-control" />
    </div>
    <div class="form-group">
        <label>Nombre</label>
        <input asp-for="Nombre" class="form-control" />
    </div>
    <div class="form-group">
        <label>Fecha de Nacimiento</label>
        <input asp-for="FechaNacimiento" type="date" class="form-control" />
    </div>
    <div class="form-group">
        <label>Fecha de Ingreso</label>
        <input asp-for="FechaIngreso" type="date" class="form-control" />
    </div>
    <div class="form-group">
        <label>Salario por Día</label>
        <input asp-for="SalarioPorDia" type="number" step="0.01" class="form-control" />
    </div>
    <div class="form-group">
        <label>Tipo de Empleado</label>
        <select asp-for="Tipo" asp-items="Html.GetEnumSelectList<Proyecto1_1_1471_0550.Models.TipoEmpleado>()" class="form-control"></select>
    </div>
    <br />
    <button type="submit" class="btn btn-success">Guardar</button>
</form>

